{
  "buildCommand": "npm run build",
  "outputDirectory": null,
  "installCommand": "npm install",
  "functions": {
    "api/**/*.{ts,js}": {
      "runtime": "@vercel/node@18"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/frontend/dist/$1"
    }
  ]
}
